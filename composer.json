{
    "type": "drupal-drush",
    "name": "drupal/marvin_behat",
    "description": "",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "drupal",
        "drush"
    ],
    "homepage": "https://drupal.org/project/marvin_behat",
    "support": {
        "issues": "https://www.drupal.org/project/issues/marvin_behat",
        "source": "https://git.drupalcode.org/project/marvin_behat"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=8.2",
        "consolidation/robo": "^4.0",
        "drupal/marvin": "3.x-dev"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "behat/mink": "^1.10",
        "behat/mink-browserkit-driver": "^2.1",
        "composer/installers": "^1.6",
        "cweagans/composer-patches": "^2.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drush/drush": "^12.1",
        "mikey179/vfsstream": "^1.6",
        "nuvoleweb/robo-config": "^3.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "phpmd/phpmd": "^2.13",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.0",
        "sweetchuck/robo-phpcs": "3.x-dev",
        "sweetchuck/robo-phpmd": "3.x-dev",
        "sweetchuck/robo-phpstan": "2.x-dev",
        "symfony/browser-kit": "^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "weitzman/drupal-test-traits": "^2.1"
    },
    "suggest": {
        "drush/drush": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Drush\\Commands\\marvin_behat\\": "Commands/marvin_behat/",
            "Drupal\\marvin_behat\\": "src/"
